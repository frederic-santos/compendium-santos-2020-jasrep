@article{kimber1990_ExploratoryDataAnalysis,
  title = {Exploratory {{Data Analysis}} for {{Possibly Censored Data From Skewed Distributions}}},
  volume = {39},
  issn = {0035-9254},
  abstract = {Some simple data analytic techniques are discussed in relation to skewed data, as might arise in reliability or survival studies, for example. Preliminary screening for outliers via outsider labelling rules is investigated and letter-value-based methods are adapted to cope with censored observations by means of the product limit estimator.},
  number = {1},
  journal = {Journal of the Royal Statistical Society. Series C (Applied Statistics)},
  doi = {10.2307/2347808},
  author = {Kimber, A. C.},
  year = {1990},
