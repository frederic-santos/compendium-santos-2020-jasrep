@article{lacostejeanson2018_ArchitectureFemoralTibial,
  title = {Architecture of the Femoral and Tibial Diaphyses in Relation to Body Mass and Composition: {{Research}} from Whole-Body {{CT}} Scans of Adult Humans},
  volume = {167},
  issn = {00029483},
  shorttitle = {Architecture of the Femoral and Tibial Diaphyses in Relation to Body Mass and Composition},
  abstract = {Objectives: Recent investigations have evaluated the influence of body composition on long bones in order to overcome the limits of body mass (BM) estimation methods and eventually lead to studying nutrition in past populations. Knowing how fat mass (FM) and fat-free mass (FFM) impact the skeleton would also enhance the understanding of mobility, activity, and locomotion derived from bone architecture. We investigated the relationship between BM and composition, and the architecture of the entire tibial and femoral diaphyses in an adult sample representative of a wide range of variation in age, BM, and composition.},
