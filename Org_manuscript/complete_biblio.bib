@article{bertsatos2019_ValidationStudyOsteometric,
  title = {Validation Study of Osteometric Techniques for Sorting Commingled Human Skeletal Remains in Archaeological Samples},
  volume = {29},
  issn = {1047482X},
  abstract = {The biological profile of human remains is fundamental for understanding the structure of past societies and their life histories. However, the assessment of the demographics of an assemblage is hindered when dealing with commingled burials. Consequently, methods for sorting commingled skeletons are essential in a bioarchaeological context. The aim of this study was to provide insight into osteometric sorting techniques when dealing with archaeological commingled skeletal remains. For the purposes of this research, Byrd and LeGarde's pair-matching osteometric sorting and regression models, which use measurements created for fragmented bones, were tested on 61 adult individuals exhumed from single burial plots from five different Greek archaeological sites. The results showed that the regression models exhibit much lower rates of true positive results (18.2\textendash{}67.6\% and 4.8\textendash{}71.7\% for the left and right side, respectively) than the pair-matching model (74.1\textendash{}89.5\%), whereas the rates of the false positive results are almost equivalent (approximately 20\textendash{}35\%). Therefore, the application of the pair-matching osteometric sorting model can provide a set of potential matches in an archaeological context. However, it is essential to utilize other techniques as well, in order to verify the results.},
  language = {en},
  number = {2},
  journal = {International Journal of Osteoarchaeology},
  doi = {10.1002/oa.2733},
  author = {Bertsatos, Andreas and Chovalopoulou, Maria-Eleni},
  year = {2019},
  pages = {253-259}
}

@article{anastopoulou2018_StatisticalMethodReassociating,
  title = {A {{Statistical Method}} for {{Reassociating Human Tali}} and {{Calcanei}} from a {{Commingled Context}}},
  volume = {63},
  issn = {00221198},
  abstract = {In a commingled context, assessing that a talus and a calcaneus correspond to the same individual could become a primary step for accurately sorting human remains. For this purpose, the lengths and widths of the trochlea, posterior calcaneal articular surface, and posterior talar articular surface were measured in 197 individuals (105 males, 92 females) from the Athens Collection. A total of 12 highly accurate equations for reassociating tali and calcanei were developed, using simple and multiple linear regression analysis and they were found to be suitable for sorting commingled human remains. Bilateral asymmetry and sex did not have an effect on the accuracy of the method.},
  language = {en},
  number = {2},
  journal = {Journal of Forensic Sciences},
  doi = {10.1111/1556-4029.13571},
  author = {Anastopoulou, Ioanna and Karakostis, Fotios A. and Borrini, Matteo and Moraitis, Konstantinos},
