@article{becker2019_EvaluatingElbowOsteoarthritis,
  title = {Evaluating Elbow Osteoarthritis within the Prehistoric {{Tiwanaku}} State Using Generalized Estimating Equations ({{GEE}})},
  issn = {00029483},
  abstract = {Objectives: Studies of osteoarthritis (OA) in human skeletal remains can come with scalar problems. If OA measurement is noted as present or absent in one joint, like the elbow, results may not identify specific articular pathology data and the sample size may be insufficient to address research questions. If calculated on a per data point basis (i.e., each articular surface within a joint), results may prove too data heavy to comprehensively understand arthritic changes, or one individual with multiple positive scores may skew results and violate the data independence required for statistical tests. The objective of this article is to show that the statistical methodology Generalized Estimating Equations (GEE) can solve scalar issues in bioarchaeological studies.},
